{
  "name": "@remotelock/react-week-scheduler",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://remotelock.github.io/react-week-scheduler/",
  "private": false,
  "sideEffects": false,
  "keywords": [
    "calendar",
    "time grid",
    "week",
    "scheduler",
    "schedule",
    "google calendar",
    "fullcalendar"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "@reach/visually-hidden": "^0.1.2",
    "@rehooks/component-size": "^1.0.2",
    "classcat": "^3.2.5",
    "date-fns": "^1.30.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "invariant": "^2.2.4",
    "lodash": "^4.17.11",
    "merge-ranges": "^1.0.2",
    "mousetrap": "^1.6.3",
    "re-resizable": "^4.11.0",
    "react-draggable": "^3.2.1",
    "react-hook-mousetrap": "^2.0.2",
    "react-spring": "7.2.10",
    "rxjs": "^6.4.0",
    "use-undo": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "resize-observer-polyfill": "^1.5.1",
    "scroll-into-view-if-needed": "^2.2.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^24.0.9",
    "@types/lodash": "4.14.120",
    "@types/mousetrap": "^1.6.1",
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/stylelint": "^9.4.2",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.1",
    "eslint-plugin-compat": "^2.7.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "gh-pages": "^2.0.1",
    "humanize-duration": "^3.18.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react": "^16.8.4",
    "react-custom-properties": "^1.2.0",
    "react-dom": "^16.8.4",
    "react-scripts": "^2.1.5",
    "resize-observer-polyfill": "^1.5.1",
    "scroll-into-view-if-needed": "^2.2.20",
    "semantic-release": "^15.13.3",
    "source-map-explorer": "^1.7.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-csstree-validator": "^1.3.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "typescript": "^3.3.3333",
    "yarnhook": "^0.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all -s lint-staged -p -c --aggregate-output pre-commit",
      "pre-push": "yarn pre-push",
      "post-merge": "yarnhook",
      "post-checkout": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "lint-staged": {
    "**/*.md": [
      "prettier --write",
      "git add"
    ],
    "**/*.{j,t}s{x,}": [
      "prettier --write",
      "git add"
    ],
    "**/*.json": [
      "prettier --write",
      "git add"
    ],
    "**/*.js{x,}": [
      "eslint"
    ]
  },
  "scripts": {
    "pre-commit": "yarn common-check",
    "lint-staged": "lint-staged",
    "check-prettier": "prettier './**/*.{t,j}s{,x}' '**/*.{css,scss,json,md}' --list-different",
    "common-check": "run-p -c --aggregate-output check-prettier check-integrity",
    "check-integrity": "yarn check --integrity",
    "check-ts": "tsc --project . --noEmit",
    "lint-ts/eslint": "eslint '**/*.ts{,x}'",
    "lint-ts": "run-p -c --aggregate-output 'lint-ts/*'",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "run-prettier": "prettier --write '*.json' 'types/**/*.ts' 'src/**/*.ts{,x}' 'src/**/*.scss'"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
